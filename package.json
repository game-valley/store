{
  "name": "game-valley-store",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "swc src -d dist",
    "start": "yarn build && NODE_ENV=production node dist/main/index.js",
    "dev": "nodemon --ext ts --exec 'yarn build && NODE_ENV=development node --inspect dist/main/index.js'",
    "test": "NODE_ENV=test jest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.172",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "git-commit-msg-linter": "^4.1.2",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": ">=10",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "class-validator": "^0.13.2",
    "reflect-metadata": "^0.1.13"
  }
}

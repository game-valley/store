{
  "name": "game-valley-store",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "swc src -d dist",
    "start": "yarn build && cross-env NODE_ENV=production node dist/main/server.js",
    "dev": "nodemon --ext ts --exec 'yarn build && cross-env NODE_ENV=development node --inspect dist/main/server.js'",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "yarn test --watch",
    "test:e2e": "yarn test --watch --silent -c jest-e2e.config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:coverage": "yarn test --coverage",
    "typeorm": "node -r '@swc/register' ./node_modules/typeorm/cli.js -d src/infra/database/postgres/config/dataSource.ts",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:create": "typeorm migration:create",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.172",
    "@swc/register": "^0.1.10",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "git-commit-msg-linter": "^4.1.2",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": ">=10",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.0.2",
    "log4js": "^6.4.6",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.3.6"
  }
}
